<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="finalapplicationGitHub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalApplication">
        <activity
            android:name="com.hop.finalapplicationGitHub.DashboardActivity"
            android:exported="true" />
        <activity
            android:name="com.hop.finalapplicationGitHub.SignUpActivity"
            android:exported="true" />
        <activity
            android:name="com.hop.finalapplicationGitHub.LoginActivity"
            android:exported="true" />
        <activity
            android:name="com.hop.finalapplicationGitHub.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>